      "path": "app/registry/blocks/logo/logo15/Logo15.vue",
      "content": "<template>\n    <section class=\"w-full py-12 md:py-24 lg:py-32 bg-white\">\n        <div class=\"container px-4 md:px-6\">\n            <!-- Header -->\n            <div class=\"flex flex-col items-center justify-center space-y-4 text-center mb-12\">\n                <div class=\"space-y-2\">\n                    <h2 class=\"text-3xl font-bold tracking-tighter sm:text-4xl md:text-5xl\">\n                        Our Enterprise Partners\n                    </h2>\n                    <p class=\"mx-auto max-w-[700px] text-gray-500 md:text-xl/relaxed lg:text-base/relaxed xl:text-xl/relaxed\">\n                        Trusted by industry leaders worldwide to deliver exceptional results\n                    </p>\n                </div>\n            </div>\n\n            <!-- Partner Carousel -->\n            <div class=\"mb-12\">\n                <Carousel\n                    :opts=\"{\n                        loop: true,\n                        align: 'end',\n                    }\"\n                    :plugins=\"[\n                        AutoScroll({\n                            playOnInit: true,\n                        }),\n                    ]\"\n                    class=\"w-full\"\n                >\n                    <CarouselContent>\n                        <CarouselItem\n                            v-for=\"partner in partners\"\n                            :key=\"partner.name\"\n                            class=\"basis-full sm:basis-1/2 md:basis-1/3 lg:basis-1/4\"\n                        >\n                            <Card class=\"p-6\">\n                                <div class=\"flex flex-col items-center space-y-4\">\n                                    <div class=\"w-30 flex items-center justify-center\">\n                                        <img\n                                            :alt=\"partner.name\"\n                                            :src=\"partner.logo\"\n                                            :height=\"32\"\n                                            class=\"w-full h-6 group-hover:scale-110 transition-transform duration-300\"\n                                        >\n                                    </div>\n                                    <div class=\"text-center space-y-1\">\n                                        <h3 class=\"font-medium\">\n                                            {{ partner.name }}\n                                        </h3>\n                                        <p class=\"text-sm text-gray-500\">\n                                            {{ partner.category }}\n                                        </p>\n                                    </div>\n                                </div>\n                            </Card>\n                        </CarouselItem>\n                    </CarouselContent>\n                    <CarouselPrevious />\n                    <CarouselNext />\n                </Carousel>\n            </div>\n\n            <!-- CTA -->\n            <div class=\"flex flex-col items-center space-y-4 text-center\">\n                <h3 class=\"text-2xl font-bold\">\n                    Ready to Join Our Partner Network?\n                </h3>\n                <p class=\"text-gray-500 max-w-[600px]\">\n                    Connect with us to explore partnership opportunities and grow your business\n                </p>\n                <div class=\"flex flex-col sm:flex-row gap-4\">\n                    <Button size=\"lg\">\n                        Become a Partner\n                    </Button>\n                    <Button\n                        size=\"lg\"\n                        variant=\"outline\"\n                    >\n                        Learn More\n                    </Button>\n                </div>\n            </div>\n        </div>\n    </section>\n</template>\n\n<script lang=\"ts\" setup>\nimport { Button } from '@/components/ui/button'\nimport { Card } from '@/components/ui/card'\nimport { Carousel, CarouselContent, CarouselItem, CarouselNext, CarouselPrevious } from '@/components/ui/carousel'\nimport AutoScroll from 'embla-carousel-auto-scroll'\n\nconst partners = [\n    {\n        name: 'Gitlab',\n        logo: 'https://cdn.svglogos.dev/logos/gitlab.svg',\n        category: 'Enterprise Software',\n        employees: '1000+',\n        rating: 4.8,\n    },\n    {\n        name: 'Vue Use',\n        logo: 'https://cdn.svglogos.dev/logos/vueuse.svg',\n        category: 'Library',\n        employees: '500+',\n        rating: 4.9,\n    },\n    {\n        name: 'Vue',\n        logo: 'https://cdn.svglogos.dev/logos/vue.svg',\n        category: 'Library',\n        employees: '200+',\n        rating: 4.7,\n    },\n    {\n        name: 'Nuxt',\n        logo: 'https://cdn.svglogos.dev/logos/nuxt.svg',\n        category: 'Development',\n        employees: '150+',\n        rating: 4.6,\n    },\n    {\n        name: 'Nextjs',\n        logo: 'https://cdn.svglogos.dev/logos/nextjs.svg',\n        category: 'Enterprise Software',\n        employees: '300+',\n        rating: 4.5,\n    },\n    {\n        name: 'Obsidian',\n        logo: 'https://cdn.svglogos.dev/logos/obsidian.svg',\n        category: 'Project Management',\n        employees: '100+',\n        rating: 4.4,\n    },\n]\n</script>\n",
      "type": "registry:block"
    }
  ]
